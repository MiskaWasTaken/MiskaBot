{
	"name": "miskabot",
	"version": "0.0.0",
	"description": "",
	"main": "dist/index.js",
	"scripts": {
		"test": "yarn run lint && yarn run build",
		"build": "yarn rimraf dist/ && yarn tsc",
		"lint": "yarn eslint --ext js,jsx,ts,tsx src",
		"start": "yarn build && node --trace-warnings -r source-map-support/register dist/index.js"
	},
	"author": "Zordlan",
	"license": "FUCKOFF",
	"private": true,
	"devDependencies": {
		"@types/module-alias": "^2",
		"@types/moment-duration-format": "^2",
		"@types/node": "^14.17.6",
		"@types/node-fetch": "^2",
		"@types/prettier": "^2.2.3",
		"@types/snekfetch": "^4",
		"@typescript-eslint/eslint-plugin": "^4.15.2",
		"@typescript-eslint/parser": "^4.15.2",
		"discord-api-types": "^0.20.2",
		"eslint": "^7.20.0",
		"prettier": "^2.2.1",
		"rimraf": "^3.0.2",
		"source-map-support": "^0.5.19",
		"typescript": "^4.2.2"
	},
	"dependencies": {
		"@discordjs/opus": "^0.5.3",
		"@discordjs/voice": "^0.5.5",
		"@distube/soundcloud": "^0.1.3",
		"@distube/spotify": "^0.4.1",
		"@iamtraction/google-translate": "^1.1.2",
		"axios": "^0.21.1",
		"chalk": "^4.1.1",
		"covidapi": "^0.5.0",
		"discord-akairo": "github:SkyBlockDev/discord-akairo",
		"discord-buttons": "^4.0.0",
		"discord-buttons-pages": "^1.1.7",
		"discord-image-generation": "^1.4.9",
		"discord.js": "^13.0.0-dev.t1628035429.00d5cee",
		"discordjs-button-pagination": "^1.0.4",
		"distube": "^3.0.0-alpha.17",
		"dotenv": "^10.0.0",
		"ffmpeg-static": "^4.4.0",
		"google-it": "^1.6.2",
		"got": "^11.8.2",
		"hmfull": "^1.2.5",
		"math.js": "^1.1.46",
		"mathjs": "^9.4.4",
		"module-alias": "^2.2.2",
		"moment": "^2.29.1",
		"moment-duration-format": "^2.3.2",
		"ms": "^2.1.3",
		"node-fetch": "^2.6.1",
		"node-gyp": "^8.1.0",
		"node-superfetch": "^0.2.3",
		"p-finally": "^3.0.0",
		"pretty-ms": "^7.0.1",
		"python": "^0.0.4",
		"quick.db": "^7.1.3",
		"sodium": "^3.0.2",
		"yarn": "npm:null@^2.0.0",
		"youtube-dl": "^3.4.1"
	},
	"prettier": {
		"tabWidth": 4,
		"singleQuote": false,
		"semi": true,
		"bracketSpacing": true,
		"arrowParens": "always",
		"printWidth": 200,
		"useTabs": true,
		"endOfLine": "lf"
	},
	"_moduleAliases": {
		"@functions": "dist/functions",
		"@extensions": "dist/extensions",
		"@src": "dist"
	},
	"eslintConfig": {
		"env": {
			"es2021": true,
			"node": true
		},
		"extends": [
			"eslint:recommended",
			"plugin:@typescript-eslint/recommended"
		],
		"parser": "@typescript-eslint/parser",
		"parserOptions": {
			"ecmaVersion": 12,
			"sourceType": "module"
		},
		"plugins": [
			"@typescript-eslint"
		],
		"ignorePatterns": [
			"dist"
		],
		"rules": {
			"@typescript-eslint/explicit-module-boundary-types": "off",
			"@typescript-eslint/no-var-requires": "off"
		}
	}
}
