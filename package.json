{
	"name": "miskabot",
	"version": "0.0.0",
	"description": "",
	"main": "dist/index.js",
	"scripts": {
		"test": "yarn run lint && yarn run build",
		"build": "yarn rimraf dist/ && yarn tsc",
		"lint": "yarn eslint --ext js,jsx,ts,tsx src",
		"start": "yarn build && node --trace-warnings -r source-map-support/register dist/index.js"
	},
	"author": "Zordlan",
	"license": "FUCKOFF",
	"private": true,
	"devDependencies": {
		"@types/module-alias": "^2",
		"@types/node": "^14.17.6",
		"@types/prettier": "^2.2.3",
		"@types/snekfetch": "^4",
		"@typescript-eslint/eslint-plugin": "^4.15.2",
		"@typescript-eslint/parser": "^4.15.2",
		"discord-api-types": "^0.20.2",
		"eslint": "^7.20.0",
		"prettier": "^2.2.1",
		"rimraf": "^3.0.2",
		"source-map-support": "^0.5.19",
		"typescript": "^4.2.2"
	},
	"dependencies": {
		"axios": "^0.21.1",
		"chalk": "^4.1.1",
		"discord-akairo": "git+https://github.com/SkyBlockDev/discord-akairo.git",
		"discord.js": "dev",
		"dotenv": "^10.0.0",
		"got": "^11.8.2",
		"hmfull": "^1.2.5",
		"module-alias": "^2.2.2"
	},
	"prettier": {
		"tabWidth": 4,
		"singleQuote": false,
		"semi": true,
		"bracketSpacing": true,
		"arrowParens": "always",
		"printWidth": 200,
		"useTabs": true,
		"endOfLine": "lf"
	},
	"_moduleAliases": {
		"@functions": "dist/functions",
		"@extensions": "dist/extensions",
		"@src": "dist"
	},
	"eslintConfig": {
		"env": {
			"es2021": true,
			"node": true
		},
		"extends": [
			"eslint:recommended",
			"plugin:@typescript-eslint/recommended"
		],
		"parser": "@typescript-eslint/parser",
		"parserOptions": {
			"ecmaVersion": 12,
			"sourceType": "module"
		},
		"plugins": [
			"@typescript-eslint"
		],
		"ignorePatterns": [
			"dist"
		],
		"rules": {
			"@typescript-eslint/explicit-module-boundary-types": "off",
			"@typescript-eslint/no-var-requires": "off"
		}
	}
}
